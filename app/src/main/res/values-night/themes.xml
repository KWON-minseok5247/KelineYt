<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.KelineYt" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/g_gray700</item>
        <item name="colorPrimaryVariant">@color/g_gray700</item>
        <item name="colorOnPrimary">@color/g_gray700</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/g_gray700</item>
        <item name="colorSecondaryVariant">@color/g_gray700</item>
        <item name="colorOnSecondary">@color/g_gray700</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/g_gray700</item>
        <item name="android:windowLightStatusBar">false</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="MediumTextViewThin">
        <item name="android:textColor">#4b4f60</item>
        <item name="android:textSize">15sp</item>
        <item name="fontFamily">@font/sf_pro_display</item>
        <item name="android:textStyle">bold</item>
    </style>
</resources>